<mvc:View controllerName="sapips.training.jsonbinding.controller.JSONBinding"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:f = "sap.ui.layout.form"
    xmlns:core = "sap.ui.core">
    <Page id="page" title="{i18n>title}">
        <Panel id="panelBindingBasics" headerText="Data Binding Basics" class="sapUiResponsiveMargin" width="auto">
            <f:SimpleForm id="BindingBasicsSimpleForm" editable="true" layout="ResponsiveGridLayout" title="Details" >
            
            </f:SimpleForm>
        </Panel>

        <Panel id="panelAddressDetails" headerText="Address Details" class="sapUiResponsiveMargin" width="auto">
            <f:SimpleForm id="AdressSimpleForm" editable="true" layout="ResponsiveGridLayout" title="Details" >
            
            </f:SimpleForm>
        </Panel>
    </Page>
</mvc:View>